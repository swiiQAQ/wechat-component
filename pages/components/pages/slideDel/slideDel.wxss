/* pages/components/pages/slideDel/slideDel.wxss */
.slideDel{
  width: 100%;
  height: 100rpx;
  border-top: 1px solid #CCC8BE;
  line-height: 100rpx;
  font-size: 26rpx;
  box-sizing: border-box;
  overflow: hidden;
  position: relative; 
}
.slideContent{
  width: 100%;
  height: 98rpx;
  z-index: 2;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
  position: absolute;
  top:0;
  /* transition: left 0.2s ease-in-out;  */
}
.del{
  z-index: -1;
  right: 0;
  background: #FF3B30;
  height: 100rpx;
  position: absolute;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
}